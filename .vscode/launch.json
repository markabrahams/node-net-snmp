    "version": "0.2.0",
    "configurations": [    
        {
            "type": "node",
            "request": "launch",
            "name": "SNMP community get",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/example/snmp-get.js",
            "args": [
                "-v", "2c",
                "-c", "public",
                "127.0.0.1",
                "1.3.6.1.2.1.31.1.1.1.6.1"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "SNMP user get",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/example/snmp-get.js",
            "args": [
                "-d",
                "-v", "3",
                "-l", "authPriv",
                "-u", "wilma",
                "-a", "sha",
                "-A", "illhavesomeauth",
                "-x", "des",
                "-X", "andsomepriv",
                "127.0.0.1",
                "1.3.6.1.2.1.1.1.0"
            ]
        },
        {
            "type": "node",
            "request": "launch",
